
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Computer {
  id Int @id @default(autoincrement())
  dpt_id Int
  group_id Int
  network_id Int
  network_ip_id Int 
  user String
  sector String
  hotsname String?
  mac String?
  processor String?
  mem String?
  hd String?
  so String?  
}

model Group {
  id Int @id @default(autoincrement())
  name String
}

model Departament {
  id Int @id @default(autoincrement())
  name String
}

model Network_address {
  id Int @id @default(autoincrement())
  address String
}

model Ip_address {
  id Int @id @default(autoincrement())
  network_id Int
  ip String
  network_ip String
  gatway String?
  dns_one String?
  dns_two String?
}

model Print {
  id Int @id @default(autoincrement())
  network_id Int
  network_ip_id String
  dpt_id Int ?
  sector String 
  hostname String ?
  model String
  serial String
  description String?
  code String?
}

model Phone {
  id Int @id @default(autoincrement())
  dpt_id Int?
  network_id Int
  network_ip_id String
  phone_number String
  sector String?
  model String?
}


enum Role {
  USER
  ADMIN
}

model Admin {
  id Int @id @default(autoincrement())
  name  String
  password String
  role Role @default(USER)
}












